  <%=form_for(constellation) do |f|%>
        <%=f.label :name%>
        <%=f.text_field :name%>
        <br><br>
        <%=f.label :image_url%>
        <%=f.text_field :image_url%>
        <br><br>
        <%=f.label :description%>
        <%=f.text_field :description%>
        <br><br>
        <%=f.label :light_years_away_from_earth%>
        <%=f.number_field :light_years_away_from_earth%>
        <br><br>
        <%=f.submit "Submit New List"%>
    <%end%>
