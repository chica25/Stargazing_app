<h1>New Stargazing</h1>

<%= form_for @stargazing do |f| %>
    <%= f.collection_select :constellation_id, Constellation.all, :id, :name %>
    <%= f.label :location %>
    <%= f.text_field :location %>
    <%= f.label :weather %>
    <%= f.number_field :weather %>
    <%= f.label :time %>
    <%= f.time_field :time %>
    <%= f.submit %>
<% end %>
