<h1>Signup</h1>

<%= form_for @astrophotographer, url: '/signup' do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>
 
    <%= f.label :Password %>
    <%= f.password_field :password %>

    <%= f.submit "Signup" %>

<% end %> <br>

<%= link_to "login", login_path %>

<%= link_to "home", root_path %>
