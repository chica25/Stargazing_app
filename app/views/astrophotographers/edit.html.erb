
<%=form_for :astrophotographer do |f|%>
    <%=f.label :profile_image%>
    <%=f.text_field :profile_image%>
    <br><br>
    <%=f.label :bio%>
    <%=f.password_field :bio%>
    <br><br>
    <% logged_in?%>
    <%=f.submit "Edit Profile"%>
    <%else%>
    <%flash[:error] = "You're not authorize to edit this page."%>
<%end%> 

